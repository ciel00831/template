<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>
	<typeAliases>
		<typeAlias type="com.tiny.user.User" alias="user" />
		<typeAlias type="com.tiny.member.Member" alias="member" />
		<typeAlias type="com.tiny.document.Document" alias="document" />
		<typeAlias type="com.tiny.comment.Comment" alias="comment" />
	</typeAliases>
</configuration>
